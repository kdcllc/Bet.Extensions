<Project>
  <!-- Reference for the msbuild tags: https://docs.microsoft.com/en-us/dotnet/core/tools/csproj -->
  <PropertyGroup>
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
    <CodeAnalysisRuleSet>$(MSBuildThisFileDirectory)betcommon.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>

  <PropertyGroup>
    <AspNetCoreVersion>[2.2.0, )</AspNetCoreVersion>
    <AspNetCoreVersion2>$(AspNetCoreVersion)</AspNetCoreVersion2>
    <EFCommonVersion>[2.2.0, )</EFCommonVersion>
    <ExtensionsVersion>[2.2.0, )</ExtensionsVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(TargetFramework)' == 'netcoreapp2.2' Or '$(TargetFramework)' == 'netstandard2.0'">
    <AspNetCoreVersion>[2.2.*, )</AspNetCoreVersion>
    <AspNetCoreVersion2>$(AspNetCoreVersion)</AspNetCoreVersion2>
    <EFCommonVersion>[2.2.*, )</EFCommonVersion>
    <ExtensionsVersion>[2.2.*, )</ExtensionsVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(TargetFramework)' == 'netcoreapp3.0' Or '$(TargetFramework)' == 'netstandard2.1'">
    <AspNetCoreVersion>3.0.0-preview9.*</AspNetCoreVersion>
    <AspNetCoreVersion2>3.0.0-*</AspNetCoreVersion2>
    <EFCommonVersion>3.0.0-preview9.*</EFCommonVersion>
    <ExtensionsVersion>3.0.0-preview9.*</ExtensionsVersion>
  </PropertyGroup>

  <ItemGroup Label="NetCore">
    <PackageReference Update="Microsoft.AspNetCore.Diagnostics.EntityFrameworkCore" Version="$(EFCommonVersion)" />
    <PackageReference Update="Microsoft.AspNetCore.Mvc.Razor.RuntimeCompilation" Version="$(AspNetCoreVersion)" />
    <PackageReference Update="Microsoft.AspNetCore.Hosting" Version="$(AspNetCoreVersion2)" />
    <PackageReference Update="Microsoft.AspNetCore.Hosting.Abstractions" Version="$(AspNetCoreVersion2)" />
    <PackageReference Update="Microsoft.AspNetCore.Http.Abstractions" Version="$(AspNetCoreVersion2)" />
    <PackageReference Update="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="$(EFCommonVersion)" />
    <PackageReference Update="Microsoft.AspNetCore.Identity.UI" Version="$(EFCommonVersion)" />
    <PackageReference Update="Microsoft.AspNetCore.Mvc" Version="$(AspNetCoreVersion2)"/>
    <PackageReference Update="Microsoft.AspNetCore.Mvc.ApiExplorer" Version="$(AspNetCoreVersion2)" />
    <PackageReference Update="Microsoft.AspNetCore.Mvc.NewtonsoftJson" Version="$(AspNetCoreVersion)" />
    <PackageReference Update="Microsoft.AspNetCore.Mvc.Testing" Version="$(AspNetCoreVersion)"/>
    <PackageReference Update="Microsoft.AspNetCore.Razor" Version="$(AspNetCoreVersion2)" />
    <PackageReference Update="Microsoft.AspNetCore.Server.Kestrel" Version="$(AspNetCoreVersion)" />
    <PackageReference Update="Microsoft.AspNetCore.StaticFiles" Version="$(AspNetCoreVersion2)" />
    <PackageReference Update="Microsoft.AspNetCore.TestHost" Version="$(AspNetCoreVersion)" />
    <PackageReference Update="Microsoft.AspNetCore.Connections.Abstractions" Version="$(AspNetCoreVersion)"/>
    <PackageReference Update="Microsoft.AspNetCore.Server.Kestrel.Core" Version="$(AspNetCoreVersion2)" />
  </ItemGroup>

  <ItemGroup Label="EF">
    <PackageReference Update="Microsoft.EntityFrameworkCore.Relational" Version="$(EFCommonVersion)" />
    <PackageReference Update="Microsoft.EntityFrameworkCore.Sqlite" Version="$(EFCommonVersion)" />
    <PackageReference Update="Microsoft.EntityFrameworkCore.SqlServer" Version="$(EFCommonVersion)" />
    <PackageReference Update="Microsoft.EntityFrameworkCore.Tools" Version="$(EFCommonVersion)"/>
  </ItemGroup>

  <ItemGroup Label="Extensions">
    <PackageReference Update="Microsoft.Extensions.Configuration" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Configuration.Binder" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Configuration.CommandLine" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Configuration.EnvironmentVariables" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Configuration.UserSecrets" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.DependencyInjection" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.DependencyInjection" Version="$(ExtensionsVersion)"/>
    <PackageReference Update="Microsoft.Extensions.DependencyInjection.Abstractions" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Diagnostics.HealthChecks" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Hosting" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Hosting.Abstractions" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Http.Polly" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Logging.Configuration" Version="$(ExtensionsVersion)"/>
    <PackageReference Update="Microsoft.Extensions.Logging.Console" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Logging.Debug" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.ObjectPool" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Options" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Options.ConfigurationExtensions" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Options.DataAnnotations" Version="$(ExtensionsVersion)" />
    <PackageReference Update="Microsoft.Extensions.Primitives" Version="$(ExtensionsVersion)" />
  </ItemGroup>

  <ItemGroup Label="Microsoft.Azure.KeyVault">
    <PackageReference Update="Microsoft.Azure.Services.AppAuthentication" Version="1.3.1"/>
    <PackageReference Update="Microsoft.Azure.KeyVault" Version="3.0.4" />
    <PackageReference Update="Microsoft.Azure.KeyVault.Core" Version="3.0.4" />
    <PackageReference Update="Microsoft.Extensions.Configuration.AzureKeyVault" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup Label="Microsoft.Azure.Storage">
    <PackageReference Update="Microsoft.Azure.Storage.Blob" Version="11.0.0" />
    <PackageReference Update="Microsoft.Azure.Storage.Queue" Version="11.0.0" />
    <PackageReference Update="Microsoft.Azure.Cosmos.Table" Version="1.0.4" />
  </ItemGroup>

  <ItemGroup Label="Unit Tests">
    <PackageReference Update="Microsoft.NET.Test.Sdk" Version="16.2.0" />
    <PackageReference Update="Moq" Version="4.13.0" />
    <PackageReference Update="xunit" Version="2.4.1" />
    <PackageReference Update="xunit.runner.visualstudio" Version="2.4.1" />
  </ItemGroup>

  <ItemGroup Label="Serilog">
    <PackageReference Update="Serilog" Version="2.8.0" />
    <PackageReference Update="Serilog.Extensions.Logging" Version="3.0.1" />
    <PackageReference Update="Serilog.Settings.Configuration" Version="3.1.0" />
    <PackageReference Update="Serilog.Sinks.ApplicationInsights" Version="3.0.4" />
    <PackageReference Update="Serilog.Sinks.AzureAnalytics" Version="4.6.0" />
    <PackageReference Update="Serilog.Sinks.Console" Version="3.1.1" />
    <PackageReference Update="Serilog.Sinks.RollingFile" Version="3.3.0" />
  </ItemGroup>

  <ItemGroup Label="Shared">
    <PackageReference Update="Certes" Version="2.3.3" />
    <PackageReference Update="Colorful.Console" Version="1.2.9" />
    <PackageReference Update="CsvHelper" Version="12.1.2" />
    <PackageReference Update="McMaster.Extensions.CommandLineUtils" Version="2.3.4"/>
    <PackageReference Update="Microsoft.AspNetCore.Diagnostics.HealthChecks" Version="2.2.0" />
    <PackageReference Update="Microsoft.AspNetCore.Mvc.Versioning.ApiExplorer" Version="4.0.0-preview8.19405.7"/>
    <PackageReference Update="Microsoft.DataPrep" Version="0.0.1.6-preview" />
    <PackageReference Update="Microsoft.ML" Version="1.3.1" />
    <PackageReference Update="Microsoft.VisualStudio.Azure.Containers.Tools.Targets" Version="1.9.5" />
    <PackageReference Update="Newtonsoft.Json" Version="12.0.2" />
    <PackageReference Update="Polly" Version="7.1.1" />
    <PackageReference Update="CronScheduler.AspNetCore" Version="1.1.2" />
  </ItemGroup>

  <ItemGroup Label="Swashbuckle">
    <PackageReference Update="Swashbuckle.AspNetCore" Version="5.0.0-rc2" />
    <PackageReference Update="Swashbuckle.AspNetCore.Swagger" Version="5.0.0-rc2" />
    <PackageReference Update="Swashbuckle.AspNetCore.SwaggerUI" Version="5.0.0-rc2" />
  </ItemGroup>

  <ItemGroup Label="SourceLink">
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-beta2-19367-01" PrivateAssets="All" />
  </ItemGroup>

  <ItemGroup Label="Analyzers">
    <PackageReference Include="StyleCop.Analyzers" Version="1.1.118" PrivateAssets="All" />
    <PackageReference Include="Microsoft.CodeAnalysis.FxCopAnalyzers" Version="2.9.4" PrivateAssets="All" />
  </ItemGroup>
</Project>
